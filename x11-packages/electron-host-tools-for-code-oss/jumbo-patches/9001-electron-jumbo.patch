+jumbo_source_set("electron_lib") {
   configs += [
     "//v8:external_startup_data",
     "//third_party/electron_node:node_external_config",
@@ -553,6 +554,22 @@ source_set("electron_lib") {
   deps += [ "//electron/build/config:generate_mas_config" ]
 
